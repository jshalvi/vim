set sta

" colors github
" colors solarized
colors darcula
set gfn=Courier_New:h12:cANSI
set gfn=Lucida_Sans_Typewriter:h11:cANSI
set gfn=Lucida_Sans_Typewriter_Regular:h13
set gfn=Inconsolata:h16
set guioptions-=rL " Get rid of left and right scrollbars
set guioptions-=m " Get rid of the menu bar
set guioptions-=T " Get rid of the Toolbar

winpos 0 0
set lines=50
set columns=175

